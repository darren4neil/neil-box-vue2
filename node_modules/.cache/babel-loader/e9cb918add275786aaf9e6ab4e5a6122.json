apse(){\n              this.$store.commit('isCollapse')\n          }\n\t    },\n        watch:{\n            headerColor:function(headerColor){\n                this.$store.commit('headerColor', headerColor)\n            }\n        }\n\t  }\n</script>\n\n\n<style scoped>\n    .headbar {\n        position: fixed;\n        top: 0;\n        right: 0;\n        z-index: 1030;\n        height: 60px;\n        border-color: rgba(180, 190, 190, 0.8);\n        border-left-width: 1px;\n        border-left-style: solid;\n    }\n    .CollapseCon, .leftBar{\n        float: left;\n    }\n    .rightBar{\n        float: right;\n    }\n    .headbar-width1{\n        left: 200px;\n    }\n    .headbar-width2{\n        left: 65px;\n    }\n    .userInfo {\n        font-size: 20px;\n        color: #fff;\n        cursor: pointer;\n    }\n    .userImg{\n        width: 40px;\n        height: 40px;\n        border-radius: 10px;\n        margin: 10px 0px 10px 10px;\n        float: right;\n    }\n    .colorI{\n        color: #000;\n        font-size: 25px;\n    }\n</style>"],"sourceRoot":"src/menu"}]}